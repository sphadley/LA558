{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch index.html",
        "type": "chrome",
        "request": "launch",
        "runtimeExecutable": "chromium",
        "runtimeArgs": [
            "--remote-debugging-port=9222", "--user-data-dir=remote-profile"
        ],
        "file": "/home/sph/git/LA558/Assignment25/index.html"
        
    },
    {
        "name": "Launch localhost with sourcemaps",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost/mypage.html",
        "sourceMaps": true,
        "webRoot": "wwwroot"
    },
    {
        "name": "Attach",
        "type": "chrome",
        "request": "attach",
        "port": 9222
    }
 ]
}